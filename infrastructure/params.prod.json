{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environment": {
      "value": "prod"
    },
    "location": {
      "value": "westeurope"
    },
    "resourcePrefix": {
      "value": "prod-euw"
    },
    "tags": {
      "value": {
        "environment": "prod",
        "application": "mystira-app",
        "owner": "mystira-team"
      }
    },
    "appServiceSku": {
      "value": "P1v3"
    },
    "storageSku": {
      "value": "Standard_RAGRS"
    },
    "cosmosServerless": {
      "value": false
    },
    "logRetentionDays": {
      "value": 90
    },
    "logDailyQuotaGb": {
      "value": -1
    },
    "acsSenderEmail": {
      "value": "DoNotReply@mystira.app"
    },
    "emailDomainName": {
      "value": "mystira.app"
    },
    "corsAllowedOrigins": {
      "value": "https://mystira.app,https://www.mystira.app"
    },
    "deployAzureBot": {
      "value": true
    },
    "botSku": {
      "value": "S1"
    },
    "enableWhatsApp": {
      "value": true
    }
  }
}
