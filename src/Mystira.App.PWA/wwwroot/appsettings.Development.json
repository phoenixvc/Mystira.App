{
  "ConnectionStrings": {
    "MystiraApiBaseUrl": "https://api.dev.mystira.app/"
  },
  "ApiConfiguration": {
    "DefaultApiBaseUrl": "https://api.dev.mystira.app/",
    "AdminApiBaseUrl": "https://admin.dev.mystira.app/",
    "Environment": "Development",
    "AvailableEndpoints": [
      { "Name": "Production", "Url": "https://api.mystira.app/", "Environment": "Production" },
      { "Name": "Staging", "Url": "https://api.staging.mystira.app/", "Environment": "Staging" },
      { "Name": "Development", "Url": "https://api.dev.mystira.app/", "Environment": "Development" },
      { "Name": "Local", "Url": "http://localhost:5260/", "Environment": "Local" }
    ],
    "AllowEndpointSwitching": true
  },
  "Features": {
    "Discord": {
      "Enabled": false
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Components.WebAssembly": "Information",
      "Mystira.PWA": "Debug"
    }
  }
}
