{
  "ConnectionStrings": {
    "MystiraApiBaseUrl": "https://api.dev.mystira.app/"
  },
  "ApiConfiguration": {
    "DefaultApiBaseUrl": "https://api.dev.mystira.app/",
    "AdminApiBaseUrl": "https://adminapi.dev.mystira.app/",
    "Environment": "Development",
    "AvailableEndpoints": [
      { "name": "Production", "url": "https://api.mystira.app/", "environment": "prod" },
      { "name": "Staging", "url": "https://api.staging.mystira.app/", "environment": "staging" },
      { "name": "Development", "url": "https://api.dev.mystira.app/", "environment": "dev" },
      { "name": "Local", "url": "https://localhost:7001/", "environment": "local" }
    ],
    "AllowEndpointSwitching": true,
    "PersistEndpointChoice": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Components.WebAssembly": "Information",
      "Mystira.PWA": "Debug"
    }
  }
}
